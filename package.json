{
  "name": "prismjs-darcula-theme",
  "version": "1.0.0",
  "description": "PrismJS Darcula syntax highlighting theme based on the JetBrains IDEs",
  "main": "darcula.css",
  "scripts": {
    "build": "node-sass --output-style compressed darcula.scss darcula.css",
    "docs": "node-sass --output-style compressed darcula.scss docs/darcula.css",
    "docs:watch": "node-sass --output-style compressed --watch darcula.scss docs/darcula.css",
    "prepack": "npm run build"
  },
  "keywords": [
    "prismjs",
    "darcula",
    "theme",
    "syntax",
    "highlight",
    "css",
    "scss",
    "style",
    "dark",
    "gatsby"
  ],
  "author": "@LucaScorpion",
  "repository": "github:LucaScorpion/prismjs-darcula-theme",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.14.1"
  }
}
